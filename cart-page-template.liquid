                            <a href="{{ item.url }}" class="cart-page__product-title text-uppercase">{{ item.product.title }}</a>
                            <p class="cart-page__product-subtitle">{{ item.product.type }}</p>
                            <h5 class="text-uppercase font-15-cart js-hex-color" data-cart-color= "{{ item.variant.title }}">{% if item.variant.title != 'Default Title' %} {{ item.variant.title }} {%endif%} </h5> 
